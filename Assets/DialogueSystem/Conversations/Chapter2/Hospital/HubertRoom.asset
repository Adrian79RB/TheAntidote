  m_Script: {fileID: 11500000, guid: f6e7c39a969bf014181dc345647a0e83, type: 3}
  m_Name: HubertRoom
  m_EditorClassIdentifier: 
  allLines:
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "\xBFDoctor? \xBFEs usted? \xBFAcaso ya es la hora del pr\xF3ximo sedante?"
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "No soy tu doctor, perdona la intromisi\xF3n. Soy Kieran, mi padre es
      el Sr. Barclay, \xE9l me dijo que viniera a verte."
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "Ah, el hijo de Jeff, la verdad es que no esperaba una visita...realmente
      no he recibido ninguna visita desde la \xFAltima vez que tu padre pas\xF3 por
      aqu\xED."
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "Quiz\xE1s tenga algo que ver con los hombres que ten\xEDa custodiando
      su puerta afuera, dec\xEDan que hab\xEDan cerrado toda la planta.\r\n"
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "Ese Luther...no pens\xE9 que pudiera ser tan mezquino. Es una posibilidad
      muchacho *cogh* *cogh*"
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "\xBFSe encuentra bien?, cuando preguntamos por usted en recepci\xF3n
      nos dijeron que se encontraba en estado cr\xEDtico."
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: " Ser\xE1 la \xFAnica verdad que hayas escuchado en todo el d\xEDa
      de hoy. \xBFPor que est\xE1s aqu\xED?"
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "Ver\xE1...mi padre lleva algunos d\xEDas sin venir por casa, y cuando
      fui a su cuarto a buscar algo que me dijera d\xF3nde pod\xEDa estar encontr\xE9
      una nota que indicaba unos viejos almacenes."
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "Cuando llegu\xE9 all\xED no estaban tan abandonados como esperaba,
      mi padre hab\xEDa montado una especie de laboratorio all\xED...en este laboratorio
      hab\xEDa una grabadora en la que se le mencionaba a usted..."
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "Con lo que pens\xE9 que, si quer\xEDa conseguir informaci\xF3n sobre
      el paradero de mi padre, primero deber\xEDa preguntarle a usted.\r\n"
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "...No te equivocas, creo saber d\xF3nde puede estar tu padre, o al
      menos d\xF3nde deber\xEDas empezar a buscar...pero\u2026\xBFest\xE1s seguro
      de querer saberlo?"
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "\xBFPor qu\xE9 lo pregunta?"
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "Porque si vas a buscar a tu padre, te estar\xE1s metiendo en la boca
      del lobo, y quiz\xE1s no puedas salir."
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 080ba70f87e7c1940bf90d98ba18dd13, type: 2}
    dialogue: "Por favor, mi padre es lo poco que me queda en la vida, si sabe algo
      de \xE9l, tiene que cont\xE1rmelo."
    listener1: {fileID: 11400000, guid: 3b82cffc8102b454086c91fc9af87910, type: 2}
    dialogue2: 
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
  - speaker: {fileID: 11400000, guid: 4f0d762d586271340859268cdbfa6430, type: 2}
    dialogue: 
    listener1: {fileID: 11400000, guid: 202b0927d4902024fa0420dbc2c2451c, type: 2}
    dialogue2: "...Eres muy joven para hablar con tanto peso en tu voz hijo...pero
      de acuerdo, te dir\xE9 lo que necesitas saber, al menos podr\xE9 irme con la
      mente tranquila."
    listener2: {fileID: 11400000, guid: 7eb4f0eccc74acd4bbf7d64e73dc475f, type: 2}
    dialogue3: 
